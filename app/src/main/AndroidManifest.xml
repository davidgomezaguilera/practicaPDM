<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practicafinalpdm">

    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticaFinalPDM">
        <activity android:name=".CambiarContra" android:theme="@style/Theme.PracticaFinalPDM.NoActionBar"></activity>
        <activity android:name=".MainActivity" android:theme="@style/Theme.PracticaFinalPDM.NoActionBar"/>
        <activity
            android:name=".login"
            android:label="@string/app_name"
            android:theme="@style/Theme.PracticaFinalPDM.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>